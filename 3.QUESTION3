import matplotlib.pyplot as plt

# Read data from the text file
with open('test.txt', 'r') as file:
    data = file.readlines()

# Extract x and y values from the data
x_values = []
y_values = []
for line in data:
    x, y = map(float, line.split())
    x_values.append(x)
    y_values.append(y)

# Plot the line
plt.plot(x_values, y_values)

# Add labels and title
plt.xlabel('X Axis')
plt.ylabel('Y Axis')
plt.title('Line Plot')

# Display the plot
plt.show()
